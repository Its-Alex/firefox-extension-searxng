{
  "author": "itsalex",
  "browser_specific_settings": {
    "gecko": {
      "id": "searxng@itsalex.fr",
      "strict_min_version": "100.0"
    },
    "gecko_android": {
      "strict_min_version": "113.0"
    }
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "encoding": "UTF-8",
      "favicon_url": "images/searxng-wordmark.svg",
      "is_default": true,
      "keyword": "searx, searxng",
      "name": "SearXNG (searxng.canine.tools) - Its-Alex",
      "search_url": "https://searxng.canine.tools/search?q={searchTerms}",
      "suggest_url": "https://searxng.canine.tools/autocompleter?q={searchTerms}"
    }
  },
  "description": "Searxng is a Firefox extension that allows you to easily define the search engine to searxng.canine.tools instance of SearXNG.",
  "homepage_url": "https://github.com/Its-Alex/firefox-extension-searxng/",
  "icons": {
    "128": "images/searxng-wordmark.svg",
    "48": "images/searxng-wordmark.svg",
    "16": "images/searxng-wordmark.svg"
  },
  "manifest_version": 2,
  "name": "SearXNG (searxng.canine.tools) - Its-Alex",
  "short_name": "SearXNG - Its-Alex",
  "version": "1.1.3"
}
